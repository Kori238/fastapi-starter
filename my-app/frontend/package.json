  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@types/react-router": "^5.1.20",
    "cypress": "^14.2.0",
    "cypress-localstorage-commands": "^2.2.4",
    "eslint-plugin-cypress": "^2.14.0",
    "prettier": "^3.0.3"
